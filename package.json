{
  "name": "demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev:env": "nodemon -r dotenv/config index.js",
    "start:production": "node index.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint --edit ",
    "test": "NODE_ENV=test tap 'test/**/**.test.js' --jobs=1",
    "test:ci": "npm run start-test-env && NODE_ENV=test tap 'test/**/**.test.js' --jobs=1",
    "test:dev": "NODE_ENV=test tap 'test/**/**.test.js' --no-coverage --jobs=1",
    "start-test-env": "NODE_ENV=test node test/helpers/scripts/start.js",
    "stop-test-env": "NODE_ENV=test node test/helpers/scripts/stop.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "8.3.1",
    "@nearform/sql": "1.9.0",
    "atob": "2.1.2",
    "btoa": "1.2.1",
    "close-with-grace": "1.1.0",
    "dataloader": "2.0.0",
    "date-fns": "2.28.0",
    "env-schema": "3.5.2",
    "fastify": "3.27.1",
    "fastify-autoload": "3.11.0",
    "fastify-error": "0.3.1",
    "fastify-plugin": "3.0.1",
    "fastify-redis": "4.3.3",
    "fluent-json-schema": "3.0.1",
    "jsonwebtoken": "8.5.1",
    "md5": "2.3.0",
    "mercurius": "9.3.2",
    "mercurius-auth": "2.0.0",
    "mercurius-cache": "1.2.0",
    "moment-timezone": "0.5.34",
    "mongoose": "6.2.1",
    "pino": "7.6.5",
    "redis-event": "0.10.0",
    "redlock": "^3.1.2",
    "truncate-html": "1.0.4",
    "undici": "^4.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "console-log-checker": "^1.0.6",
    "eslint": "8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "^7.0.4",
    "lint-staged": "12.3.3",
    "nodemon": "2.0.15",
    "pino-pretty": "^7.5.1",
    "prettier": "2.5.1",
    "tap": "15.1.6"
  },
  "lint-staged": {
    "*/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "tap": {
    "check-coverage": true,
    "branches": 50,
    "lines": 70,
    "functions": 70,
    "statements": 70
  }
}
